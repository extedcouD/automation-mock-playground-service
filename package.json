{
  "name": "ondc-gcr-reader-poc",
  "version": "1.0.0",
  "description": "poc for ondc gcr reader side",
  "license": "ISC",
  "author": "extedcouD",
  "type": "commonjs",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec tsx src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:multi-instance": "docker-compose -f src/test/E2E/docker-compose.multi-instance.yml up -d && npm test -- --testNamePattern='Docker Multi-Instance' && docker-compose -f src/test/E2E/docker-compose.multi-instance.yml down -v",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@ondc/automation-mock-runner": "^1.3.5",
    "ajv": "^8.17.1",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^3.0.1",
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "ioredis": "^5.9.2",
    "jsonpath": "^1.1.1",
    "ondc-crypto-sdk-nodejs": "^2.1.1",
    "pino": "^9.14.0",
    "pino-http": "^11.0.0",
    "pino-loki": "^2.6.0",
    "pino-pretty": "^12.1.0",
    "prom-client": "^15.1.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/jsonpath": "^0.2.4",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "@typescript-eslint/utils": "^8.47.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.5",
    "tsx": "^4.20.6"
  }
}
